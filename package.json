    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@google-cloud/vision": "^2.4.0",
    "aws-sdk": "^2.988.0",
    "axios": "^0.21.4",
    "file-type": "^16.5.3",
    "formidable": "^1.2.2",
    "fs": "0.0.1-security",
    "husky": "^7.0.2",
    "mongoose": "^6.0.5",
    "next": "latest",
    "next-auth": "^3.29.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^27.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "mongodb": "^3.5.9"
  }
